<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.hospitalrestcore.inventory">

    <class name="InventoryStoreIssuePatientDetail"
           table="inventory_store_issue_patient_detail" batch-size="500">

        <id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native" />
        </id>

        <property name="patientIdentifier" column="patient_identifier" />
        <property name="patientName" column="patient_name" />
        <property name="category" column="category" />
        <property name="age" column="age" />
        <property name="createdDate" column="created_on" />
        <property name="deleted" column="deleted" />

    </class>
</hibernate-mapping>