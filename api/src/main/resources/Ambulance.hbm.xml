<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.hospitalrestcore.billing">

	<class name="Ambulance" table="billing_ambulance" batch-size="500">

		<id name="ambulanceId" type="int" column="ambulance_id"
			unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="name" column="name" />
		<property name="description" column="description" length="255" />
		<property name="createdDate" column="created_date"  />
	    <many-to-one name="createdBy" column="created_by"  class="org.openmrs.User" foreign-key="ambulance_created_by__fk"/>
		<property name="lastModifiedDate" column="lastmodified_date"  />
	    <many-to-one name="lastModifiedBy" column="lastmodified_by"  class="org.openmrs.User" foreign-key="ambulance_lastmodified_by__fk"/>
		<property name="retired" column="retired"  />
		<property name="retiredDate" column="retired_date"  />
		<many-to-one name="retiredBy" column="retired_by"  class="org.openmrs.User" foreign-key="ambulance_retired_by__fk"/>
		<property name="uuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />
	</class>
</hibernate-mapping>
