<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping
	package="org.openmrs.module.hospitalrestcore.billing">

	<class name="TenderBillItem" table="billing_tender_bill_item"
		batch-size="500">

		<id name="tenderBillItemId" type="int" column="tender_bill_item_id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="name" column="name" />
		<property name="amount" column="amount" />
		<property name="unitPrice" column="unit_price" />
		<property name="quantity" column="quantity" />
		<many-to-one name="tender" lazy="false" column="tender_id"
			class="org.openmrs.module.hospitalrestcore.billing.Tender"
			foreign-key="fk_tenderbillitem_tender" />
		<many-to-one name="tenderBill" column="tender_bill_id"
			class="org.openmrs.module.hospitalrestcore.billing.TenderBill"
			foreign-key="fk_tenderbillitem_tenderbill" />
		<property name="createdDate" column="created_date" />
		<many-to-one name="createdBy" column="created_by"
			class="org.openmrs.User"
			foreign-key="billing_tender_bill_item_created_by_fk" />
		<property name="deleted" column="deleted" />
		<property name="deletedDate" column="deleted_date" />
		<many-to-one name="deletedBy" column="deleted_by"
			class="org.openmrs.User"
			foreign-key="billing_tender_bill_item_deleted_by_fk" />
		<property name="lastModifiedDate" column="last_modified_date" />
		<many-to-one name="lastModifiedBy"
			column="last_modified_by" class="org.openmrs.User"
			foreign-key="billing_tender_bill_item_lastmodified_by__fk" />
		<property name="retired" column="retired" />
		<property name="retiredDate" column="retired_date" />
		<many-to-one name="retiredBy" column="retired_by"
			class="org.openmrs.User"
			foreign-key="billing_tender_bill_item_retired_by_fk" />
		<property name="uuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />
		<property name="voided" column="voided" />
		<property name="voidedDate" column="voided_date" />
		<many-to-one name="voidedby" column="voided_by"
			class="org.openmrs.User"
			foreign-key="billing_tender_bill_item_voidedby" />
	</class>
</hibernate-mapping>
